<div class="container mt-4 mb-5">
    <div class="d-flex justify-content-center row">
        <div class="col-md-8">
            <div class="feed p-2">
              <% @users.each do |user| %>
                <% if current_user.can_request?(user) %>
                  <%= render 'user', user: user %>
                <% end %>
              <% end %>
            </div>
        </div>
    </div>
</div>